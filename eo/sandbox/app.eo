+package sandbox
+alias sandbox.fibonacci
+alias org.eolang.io.stdout

[args] > app
  seq > @
    *
      stdout
        args.at 0
      stdout
        "\nFibonacci(6) = "
      stdout
        48.plus
          fibonacci 6
      TRUE
